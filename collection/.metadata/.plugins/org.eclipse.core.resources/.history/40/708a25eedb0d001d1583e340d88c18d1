package Streams.flateMap;

import java.util.Arrays;

class Student {
	String name;
	int score;

	public Student(String name, int score) {
		super();
		this.name = name;
		this.score = score;
	}

	public String getName() {
		return this.name;
	}

	public int getScore() {
		return this.score;

	}

}

public class ParallelStream {

	public static void main(String[] args) {
		Arrays.asList(new Student("ram", 90), new Student("Sita", 60), new Student("murli", 89));
	}

}
