package com.jtc.C1;

public class GraterThree2 {

	public static void main(String[] args) {
	int a=200,b=300,c=400;
	if(a>b) {
		
		if(a>c) {
			System.out.println("a is Grater");
		}
		else {
			System.out.println("c is Grater");
			
		}
	}
	else if (b>c) {
		System.out.println("b is Grater");
		
	}
	else {
		
		System.out.println("c is Grater");
	}

	}

}
